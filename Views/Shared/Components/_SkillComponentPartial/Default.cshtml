@model List<MyPortfolio.DAL.Entities.Skill>

<div class="col-six tab-full right">
    <h3>Programlama Dilleri</h3>

    <ul class="skill-bars">
        @foreach(var item in Model)
        {
            string x ="progress percent"+Convert.ToString(item.Value);
            <li>
                <div class="@x"><span>@item.Value</span></div>
                <strong>@item.Title</strong>
            </li>
        }
    </ul>
</div>